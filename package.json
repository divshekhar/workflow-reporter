{
  "name": "workflow-reporter",
  "version": "1.0.0",
  "private": true,
  "description": "A GitHub app to report failed workflow job actions and custom report message for the job.",
  "author": "Divyanshu Shekhar <imdshekhar@gmail.com>",
  "license": "ISC",
  "homepage": "https://github.com/divshekhar/workflow-reporter",
  "keywords": [
    "probot",
    "github",
    "probot-app"
  ],
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "nodemon --exec \"npm start\"",
    "start": "npm run build && probot run ./lib/index.js",
    "lint": "standard **/*.ts --fix",
    "test": "jest && standard **/*.ts",
    "test:watch": "jest --watch --notify --notifyMode=change --coverage"
  },
  "dependencies": {
    "probot": "^12.2.1"
  },
  "devDependencies": {
    "@types/jest": "^26.0.19",
    "@types/node": "^14.14.19",
    "jest": "^26.6.3",
    "nock": "^13.0.5",
    "nodemon": "^2.0.15",
    "smee-client": "^1.2.2",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3"
  },
  "engines": {
    "node": ">= 10.13.0"
  }
}
